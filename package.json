{
  "name": "next-website",
  "version": "1.0.0",
  "description": "A project for next.js",
  "main": "src/pages/_app.page.tsx",
  "private": true,
  "keywords": [
    "webpack",
    "react",
    "typescript",
    "scss",
    "nextjs"
  ],
  "author": "justin",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "基础命令": "------",
    "server": "node --max_old_space_size=3072 server.js",
    "build": "next build",
    "测试环境": "------",
    "server:dev": "env-cmd -e server:dev -r ./env-cmdrc.js yarn server",
    "build:dev": "env-cmd -e build:dev:ayx -r ./env-cmdrc.js yarn build",
    "start:dev": "env-cmd -e start:dev -r ./env-cmdrc.js yarn server",
    "预发环境": "------",
    "server:release": "env-cmd -e server:release -r ./env-cmdrc.js yarn server",
    "build:release": "env-cmd -e build:release -r ./env-cmdrc.js yarn build",
    "start:release": "env-cmd -e start:release -r ./env-cmdrc.js yarn server",
    "生产环境": "------",
    "server:prod": "env-cmd -e server:prod -r ./env-cmdrc.js yarn server",
    "build:prod": "env-cmd -e build:prod -r ./env-cmdrc.js yarn build",
    "start:prod": "env-cmd -e start:prod -r ./env-cmdrc.js yarn server",
    "打包分析": "------",
    "analyze": "env-cmd -e analyze -r ./env-cmdrc.js yarn build",
    "lint命令": "------",
    "lint": "next lint"
  },
  "dependencies": {
    "antd": "^4.16.13",
    "js-base64": "^3.7.2",
    "js-next-optimized-images": "^3.0.0-canary.11",
    "next": "^11.1.2",
    "next-pwa": "^5.4.0",
    "next-videos": "^1.4.0",
    "react": "17.0.2",
    "react-cool-dimensions": "^2.0.7",
    "react-dom": "17.0.2",
    "sass": "^1.43.4",
    "sharp": "^0.29.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.3",
    "@types/node": "16.11.7",
    "@types/react": "17.0.34",
    "babel-plugin-import": "^1.13.3",
    "env-cmd": "^10.1.0",
    "eslint": "7",
    "eslint-config-next": "12.0.3",
    "express": "^4.17.1",
    "less-vars-to-js": "^1.3.0",
    "lru-cache": "^6.0.0",
    "next-compose-plugins": "^2.2.1",
    "next-plugin-antd-less": "^1.5.2",
    "open": "^8.4.0",
    "path-to-regexp": "^6.2.0",
    "prettier": "^2.4.1",
    "typescript": "4.4.4"
  }
}
